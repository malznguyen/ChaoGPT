# ChaoGPT Environment Configuration
# Copy this file to .env.local and fill in your values

# V98Store API Configuration (REQUIRED)
# Get your API key from v98store.com
V98STORE_API_KEY=your_api_key_here
V98STORE_API_URL=https://v98store.com

# Application Configuration
NEXT_PUBLIC_APP_URL=http://localhost:3000

# Rate Limiting (optional)
# Set to false to disable rate limiting during development
RATE_LIMIT_ENABLED=true

# Chaos Level (optional)
# Base chaos level for the application (0-100)
CHAOS_LEVEL=100

# Telemetry (optional)
# Enable telemetry for monitoring
ENABLE_TELEMETRY=true

# Node Environment
NODE_ENV=development
